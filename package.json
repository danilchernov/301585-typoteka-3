{
  "name": "typoteka",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "test": "npm run eslint && npm run jest",
    "jest": "cross-env NODE_ENV=test jest",
    "eslint": "eslint --ext .js src/",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier  src/ --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run eslint:fix",
    "start-back-server": "cross-env NODE_ENV=production node -r dotenv/config ./src/service/service.js --server",
    "start-back-server:debug": "cross-env NODE_ENV=development nodemon -r dotenv/config ./src/service/service.js --server",
    "start-front-server": "nodemon -r dotenv/config ./src/express/express",
    "start:parallel": "concurrently \"npm run start-back-server:debug\" \"npm run start-front-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danilchernov/301585-typoteka-3.git"
  },
  "author": "Danil Chernov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danilchernov/301585-typoteka-3/issues"
  },
  "homepage": "https://github.com/danilchernov/301585-typoteka-3#readme",
  "devDependencies": {
    "eslint": "8.9.0",
    "eslint-config-htmlacademy": "0.6.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "pino-pretty": "7.6.0",
    "prettier": "2.6.1",
    "pug-lint": "2.6.0",
    "sqlite3": "5.0.6",
    "supertest": "6.2.2"
  },
  "dependencies": {
    "axios": "0.26.1",
    "bcrypt": "5.0.1",
    "chalk": "4.1.2",
    "command-line-usage": "6.1.1",
    "concurrently": "7.1.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-session": "1.17.3",
    "joi": "17.6.0",
    "multer": "1.4.4",
    "nanoid": "3.3.1",
    "nodemon": "2.0.15",
    "pg": "8.7.3",
    "pino": "7.9.2",
    "pug": "3.0.2",
    "sequelize": "6.19.0"
  }
}
